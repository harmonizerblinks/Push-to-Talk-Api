{
    "version": 2,
    "builds": [{ "src": "src/index.js", "use": "@now/node-server" }],
    "routes": [{
            "src": "/api/users",
            "dest": "/src/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        },
        {
            "src": "/api",
            "dest": "/src/index.js",
            "methods": ["GET"]
        },
        {
            "src": "/app",
            "dest": "/src/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        },
        {
            "src": "/api/file",
            "dest": "/src/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        },
        {
            "src": "/api/setup",
            "dest": "/src/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}